{
    "version": 2,
    "builds": [
        {
            "src": "**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/about-us",
            "dest": "/about-us.html"
        },
        {
            "src": "/(.*)\\.html",
            "dest": "/$1.html"
        },
        {
            "src": "/js/(.*)",
            "dest": "/js/$1"
        },
        {
            "src": "/styles/(.*)",
            "dest": "/styles/$1"
        },
        {
            "src": "/image/(.*)",
            "dest": "/image/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}